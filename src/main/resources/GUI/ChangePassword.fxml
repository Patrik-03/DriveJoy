<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane stylesheets="@style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ChangePassword">
   <children>
      <ImageView fitHeight="371.0" fitWidth="216.0" layoutX="-2.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@greencarL.png" />
         </image></ImageView>
      <Button id="button" layoutX="126.0" layoutY="187.0" mnemonicParsing="false" onAction="#changePassword" stylesheets="@style.css" text="Change">
         <font>
            <Font name="Segoe UI" size="12.0" />
         </font></Button>
      <PasswordField id="textfield" fx:id="current" layoutX="23.0" layoutY="98.0" onAction="#changePassword" prefWidth="160.0" promptText="Current password" stylesheets="@style.css">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font></PasswordField>
      <PasswordField id="textfield" fx:id="newp" layoutX="23.0" layoutY="137.0" onAction="#changePassword" prefWidth="160.0" promptText="New password" stylesheets="@style.css">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font></PasswordField>
      <Button id="button" layoutX="23.0" layoutY="187.0" mnemonicParsing="false" onAction="#close" stylesheets="@style.css" text="Close" />
   </children>
</AnchorPane>
